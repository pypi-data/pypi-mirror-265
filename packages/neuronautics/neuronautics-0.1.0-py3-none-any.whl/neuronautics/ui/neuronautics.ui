/* This style is for the QProgressBar */
QProgressBar {
    border: 1px solid #b2b2b2;
    border-radius: 5px;
    background-color: #f0f0f0;
    text-align: center;
}

/* This style is for the chunk that fills the progress */
QProgressBar::chunk {
    background-color: #007BFF;
    width: 10px; /* Change the width to control the chunk's thickness */
}

/* Style the QTabWidget */
QTabWidget {
    background-color: #f5f5f5;
    border: 1px solid #ccc;
    border-radius: 5px;
}

/* Style the QTabBar (the tabs) */
QTabBar::tab {
    background-color: #eee;
    border: 1px solid #ccc;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    padding: 5px 10px;
    margin: 0;
}

QTabBar::tab:selected {
    background-color: #fff;
    border-bottom-color: #fff;
}

/* Style the content area */
QTabWidget::pane {
    background-color: #fff;
    border: 1px solid #ccc;
    border-top: none;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
}

