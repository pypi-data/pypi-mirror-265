
<TrackerBioCardioStatsScreen>:
    name: "tracker_bio_cardio_stats"

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        padding: 5
        spacing: 10

        Image:
            id: bp_plot_image
            texture: root.bp_plot

        Image:
            id: hr_plot_image
            texture: root.hr_plot
            center_x: self.parent.center_x
            center_y: self.parent.center_y


